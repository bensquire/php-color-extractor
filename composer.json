    "keywords": ["image", "php", "color", "colour", "extract"],
    "homepage": "https://github.com/bensquire/php-color-extractor",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Squire",
            "email": "b.squire@gmail.com",
            "homepage": "https://github.com/bensquire/php-color-extractor"
        }
    ],
    "support": {
        "issues": "https://github.com/bensquire/php-color-extractor/issues",
        "wiki": "https://github.com/bensquire/php-color-extractor/wiki"
    },
    "require": {
